#include "holberton.h"
#include <stdlib.h>
#include <stdio.h>

/**
 * memClear - Frees memory
 * @s: Stuff
 * Return: Nothing
 */

char *argstostr(int ac, char **av)
{
	int i;
	char *str;

	str = malloc(sizeof(char *) * (ac + 1));

	if (str == NULL || ac == 0)
	{
		return (NULL);
	}

	for(i = 0; i < ac; i++)
	{
		str[i] = *av[i] + '\n';
	}
	str[i] = '\0';

	return (str);
}
